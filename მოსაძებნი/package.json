{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "verify:env": "node backend/scripts/verifyEnv.js",
    "dev:backend": "NODE_ENV=development PORT=5002 node backend/index.js",
    "dev:frontend": "node ./scripts/run-vite-dev.mjs",
    "dev": "bash scripts/port-cleanup.sh && concurrently --names backend,frontend --restart-tries 99 --restart-after 2000 -c magenta,cyan -- \"npm:dev:backend\" \"npm:dev:frontend\"",
    "health": "node check-services.js",
    "build": "tsc -b && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "npx vite preview --port 5000 --host 0.0.0.0",
    "backend": "cd backend && node index.js",
    "start:backend": "cd backend && PORT=5002 node index.js",
    "start:frontend": "node ./scripts/run-vite-dev.mjs",
    "start": "bash scripts/port-cleanup.sh && concurrently --names backend,frontend --restart-tries 5 --restart-after 2000 -c magenta,cyan \"npm:start:backend\" \"npm:start:frontend\"",
    "start:dev": "npm run start:backend & npm run start:frontend",
    "test-integration": "node test_system_integration.js",
    "test-phoenix": "node test_gurulo_phoenix.js",
    "test:georgian-grammar": "node scripts/testGeorgianGrammar.js",
    "test:autoimprove": "node --test backend/__tests__/auto_improve_run_store.test.js",
    "test:ai:smoke": "node --test tests/ai-chat/smoke.test.js",
    "test:ai:fallback": "node --test tests/ai-chat/fallback.test.js",
    "test:ai:sse-smoke": "node tests/ai-chat/sse-smoke.js",
    "test:ai:all": "npm run test:ai:smoke && npm run test:ai:fallback",
    "system-health": "node system-health-check.js",
    "diagnose:ports": "node scripts/resolve-port-conflicts.js",
    "init-backup": "node scripts/init-backup-system.js",
    "test-backup": "node scripts/test-backup-system.js",
    "backup-now": "curl -X POST http://localhost:5002/api/ai/backup-system/backup/full",
    "backup-status": "curl http://localhost:5002/api/ai/backup-system/status",
    "check:secrets-required": "node scripts/checkSecretsRequiredStatus.js"
  },
