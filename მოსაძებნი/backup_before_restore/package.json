{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "verify:env": "node backend/scripts/verifyEnv.js",
    "dev:backend": "cd backend && PORT=5002 node index.js",
    "dev:frontend": "vite --port 5000 --host 0.0.0.0",
    "dev:ai": "cd ai-service && PORT=5001 HOST=0.0.0.0 node server.js",
    "dev": "concurrently -k -n backend,frontend,ai -c magenta,cyan,green \"npm:dev:backend\" \"npm:dev:frontend\" \"npm:dev:ai\"",
    "health": "node check-services.js",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 5000 --host 0.0.0.0",
    "backend": "cd backend && node index.js",
    "start:backend": "cd backend && PORT=5002 node index.js",
    "start:ai": "cd ai-service && PORT=5001 HOST=0.0.0.0 node server.js",
    "start:frontend": "vite preview --port 5000 --host 0.0.0.0",
    "start": "concurrently -k -n backend,ai,frontend -c magenta,green,cyan \"npm:start:backend\" \"npm:start:ai\" \"npm:start:frontend\"",
    "start:dev": "npm run start:backend & npm run start:frontend"
  },
